{
    "name": "Chrome wallpaper",
    "description": "Changes wallpaper",
    "version": "1.0.0",
    "manifest_version": 2,
    "icons": {},
    "background": {
        "scripts": [
            "./background.js"
        ],
        "persistent": true
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "permissions": [
        "<all_urls>", "webRequest","webRequestBlocking",
        "tabs",
        "notifications",
        "http://*/",
        "https://*/",
        "identity", "identity.email"
    ]
}